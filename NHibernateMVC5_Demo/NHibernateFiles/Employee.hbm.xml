<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="NHibernateMVC5_Demo" namespace="NHibernateMVC5_Demo.Models">
  <class name="Employees" table="Employees" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="int">
      <generator class="native" />
    </id>
    <property name="Name"  />
    <property name="Position" />
    <property name="Office" />
    <property name="Age"></property>
    <property name="StartDate"></property>
    <property name="Salary"></property>
    <property name="DepartmentId"></property>
  </class>
</hibernate-mapping>
